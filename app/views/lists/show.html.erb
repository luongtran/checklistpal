<% title "Tudli - Make To Do Lists for Everything" %><% if current_user %>    <div class="section clearfix">      <div class="pull-right">        <div id="quickmenu">          <div class="clearfix">            <a href="<%= root_path %>" class="pull-left" title="Create new list">+New</a>            <%= link_to_function("PRINT", "javascript:print()", html_options={:class => "pull-left", :target => "_blank", :title => "Print this page"}) %>            <%= link_to '+ INVITE', find_invite_path(@list.id), :class => "pull-left", :title => "Invite user to your list" %>            <a href="<%= my_list_path %>" class="pull-left" title="My list and friend list">MY LISTS</a>            <a href="<%= list_path(@list, :format => "pdf") %> " class="pull-left">PDF</a>          </div>        </div>      </div>      <div class="pull-left">        <div id="logo">          <span class="list-name" data-url="<%= edit_list_path(@list) %>">&nbsp;<%= @list.name %></span>          <a href="javascript:;" class="logo-editbt" title="Edit list name">(Edit)</a>          <label class="alert alert-error hidden editlb"></label>        </div>      </div>    </div><% else %>    <div class="section clearfix">      <div class="pull-right">        <div id="quickmenu">          <div class="clearfix">            <a href="#saveform" data-toggle="modal" data-remote="true" class="pull-left" title="Save to my lists">SAVE</a><a href="<%= root_path %>" class="pull-left" title="Create new list">+            NEW</a><%= link_to_function("PRINT", "javascript:print()", html_options={:class => "pull-left", :target => "_blank", :title => "Print this page"}) %>          </div>        </div>      </div>      <div class="pull-left">        <div id="logo">          <%= image_tag "free-checklist-tool.png" %>        </div>      </div>    </div><% end %><div class="section clearfix">  <div id="todolistsearch">    <div class="searchbox">      <%= form_for [@list, @list.tasks.new] do |form| %>          <div class="row-fluid">            <%= form.text_field :description, :class => "input-block-level searchboxtodolist", :required => true %>            <%= form.submit :value => "add +", :class => "btn btn-primary hidden-phone", :title => "Add new task" %>            <%= form.submit :value => "+", :class => "btn btn-primary visible-phone" %>          </div>          <div class="alert-error" id="error-message"></div>          </div>      <% end %>      </div>  <div class="todolist">    <div class="row-fluid">            <span class="span12">              <div class="notice" id="notice">              </div>              <ul class="unstyled" id="list-items" data-update-url="<%= sort_tasks_path %>">                <%= render :partial => "lists/items" %>              </ul>            </span>    </div>  </div></div><!-- Modal save form --><div class="modal hide fade" id="saveform" style="width:auto;">  <div class="modal-header ">    <h4 class="text-success">Please Sign in or <%= link_to "Signup", signup_options_path %>      to save to your lists</h4>  </div>  <div class="modal-body">    <%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => 'clearfix'}) do |f| %>        <div class="field">          <label>Email</label>          <%= f.email_field :email, :placeholder => "email", :autofocus => true %>          <%= f.hidden_field :list_save_id, value: @list.id %>        </div>        <div class="field">          <label>Password</label>          <%= f.password_field :password, :placeholder => "password" %>        </div>        </div>        <div class="modal-footer">          <input type="submit" value="Sign In" class="btn btn-primary">          <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>        </div>    <% end %>    </div><!-- Email form --><!--<div class="modal hide fade" id="emailform" tabindex="-1" data-width="200">--><!--<div class="modal-header">--><!--<button type="button" class="close" data-dismiss="modal" aria-hidden="true">X</button>--><!--<h3>Share list to your friends</h3>--><!--</div>--><!--<div class="modal-body text-center">--><!--<span class='st_sharethis_large' displayText='ShareThis'></span>--><!--<span class='st_facebook_large' displayText='Facebook'></span>--><!--<span class='st_twitter_large' displayText='Tweet'></span>--><!--<span class='st_linkedin_large' displayText='LinkedIn'></span>--><!--<span class='st_pinterest_large' displayText='Pinterest'></span>--><!--<span class='st_email_large' displayText='Email'></span>--><!--</div>--><!--<div class="modal-footer">--><!--<%#*<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>%>--><!--<button type="button" data-dismiss="modal" class="btn btn-success">Close</button>--><!--</div>--><!--</div>--><script>    $("a.bookmark").click(function (e) {        e.preventDefault(); // this will prevent the anchor tag from going the user off to the link        var bookmarkUrl = this.href;        var bookmarkTitle = this.title;        if (navigator.userAgent.toLowerCase().indexOf('chrome') > -1) {            alert("This function is not available in Google Chrome. Click the star symbol at the end of the address-bar or hit Ctrl-D (Command+D for Macs) to create a bookmark.");        } else if (window.sidebar) { // For Mozilla Firefox Bookmark            window.sidebar.addPanel(bookmarkTitle, bookmarkUrl, "");        } else if (window.external || document.all) { // For IE Favorite            window.external.AddFavorite(bookmarkUrl, bookmarkTitle);        } else if (window.opera) { // For Opera Browsers            $("a.bookmark").attr("href", bookmarkUrl);            $("a.bookmark").attr("title", bookmarkTitle);            $("a.bookmark").attr("rel", "sidebar");        } else { // for other browsers which does not support            alert('Your browser does not support this bookmark action');            return false;        }    });</script>