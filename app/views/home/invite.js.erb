<% if @has_list_users %>
    $("#list_invites").html('<%= escape_javascript(render 'list_invites') %>')
<% else %>
    <% if @has_over_connect %>
        $("#list_invites").text("You have to upgrade account to invite more people. You already reached the limitation connections ");
        $("#list_invites").addClass("alert alert-success");
    <% else %>
      <% if @success %>
      $("#list_invites").text("Invitation sent to <%= @user.email %>");
      $("#list_invites").addClass("alert alert-success");
        <% else %>
          $("#list_invites").text("No result match");
        <% end %>
    <% end %>
<% end %>